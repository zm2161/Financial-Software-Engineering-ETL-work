{
    "feature_args": {
        "-input": {
            "dest": "input_path",
            "help": "Input data path",
            "required": "True"
        },
        "-output": {
            "dest": "output_path",
            "help": "Output data path",
            "required": "True"
        },
        "-mapping": {
            "dest": "mapping_path",
            "help": "Mapping data path",
            "required": "True"
        }
    },
    "extraction": {
        "description": "Zhuoran Ma",
        "input": {
            "read": {
                "file_type": "csv",
                "separator": ",",
                "apply_dtype": {
                    "Date": "date",
                    "Open": "float",
                    "High": "float",
                    "Low": "float",
                    "Close": "float",
                    "Volume": "float",
                    "Adj Close": "float"
                }
            },
            "plugin": null
        },
        "mapping": {
            "read": {
                "file_type": "csv",
                "skip_rows": 0,
                
                "sheet_name": 0,
                "apply_dtype": {
                    "Date": "date",
                    "Top1": "str",
                    "Top2": "str",
                    "Top3": "str",
                    "Top4": "str",
                    "Top5": "str"
                }
            },
            "left_on": [
                "Date"
            ],
            "right_on": [
                "Date"
            ],
            "plugin": null
        },
        "output": {
            "write": {
                "file_type": "csv",
                "mode": "new",
                "col_rename": {
                    "Adj Close": "Adj_Close"
                }
            },            
            "plugin": null
        }
    }
}